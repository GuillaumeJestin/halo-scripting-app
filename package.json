{
  "name": "halo-scripting-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.16.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "concurrently": "6.4.0",
    "electron": "16.0.4",
    "electron-builder": "22.7.0",
    "electron-is-dev": "2.0.0",
    "electronmon": "2.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flow-renderer": "9.7.0",
    "react-scripts": "4.0.3",
    "sass": "1.26.10",
    "typescript": "4.5.2",
    "wait-on": "6.0.0"
  },
  "scripts": {
    "start": "concurrently \"yarn react-serve\" \"wait-on http://localhost:3000 && npm run electron-start\"",
    "build": "yarn react-build && npm run electron-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "yarn react-build && npm run electron-release",
    "electron-start": "electronmon .",
    "electron-build": "electron-builder",
    "electron-release": "electron-builder --publish=always",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-serve": "yarn react-start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron-main.js",
  "homepage": "./"
}
